{
  "name": "next-back",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:pusher:dev": "nest start pusher --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --clearCache && cross-env NODE_ENV=test jest --force-exit --detectOpenHandles ",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "jest --clearCache && cross-env NODE_ENV=test jest --coverage --force-exit --detectOpenHandles",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles --force-exit --runInBand",
	"test:e2e:cov": "jest --clearCache && cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --detectOpenHandles --force-exit"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^3.6.1",
    "@liaoliaots/nestjs-redis": "^9.0.5",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/platform-express": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "ioredis": "^5.3.2",
    "mongodb-memory-server": "^8.13.0",
    "mongoose": "^7.2.0",
    "mqtt": "^4.3.7",
    "openpgp": "^5.9.0",
    "pg": "^8.11.1",
    "rxjs": "^7.2.0",
    "toml": "^3.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@golevelup/nestjs-testing": "^0.1.2",
    "@grpc/grpc-js": "^1.8.16",
    "@grpc/proto-loader": "^0.7.7",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/microservices": "^10.0.3",
    "@nestjs/platform-socket.io": "^10.0.3",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nestjs/websockets": "^10.0.3",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.194",
    "@types/mockjs": "^1.0.7",
    "@types/mqtt": "^2.5.0",
    "@types/node": "18.15.11",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "amqp-connection-manager": "^4.1.13",
    "amqplib": "^0.10.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "prettier": "^2.3.2",
    "redis-memory-server": "^0.7.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!lodash-es)"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "modulePathIgnorePatterns": [
      "libs/interface/",
      "<rootDir>/.*\\.dto.ts",
      "<rootDir>/.*\\.module.ts",
      "<rootDir>/.*\\.middleware.ts",
      "<rootDir>/.*\\.schema.ts",
      "node_modules"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "!**/tsconfig.*.json",
      "!libs/common/src/*.decorator.ts",
      "!libs/{middleware/**,schemas/**}/*.ts",
      "!libs/mock/src/*.ts",
      "!libs/**/index.ts",
      "!libs/**/{db,jwt,cache}.module.ts",
      "!src/**/*.module.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/response(|/.*)$": "<rootDir>/libs/response/src/$1",
      "^@app/errors(|/.*)$": "<rootDir>/libs/errors/src/$1",
      "^@app/cache(|/.*)$": "<rootDir>/libs/cache/src/$1",
      "^@app/config(|/.*)$": "<rootDir>/libs/config/src/$1",
      "^@app/db(|/.*)$": "<rootDir>/libs/db/src/$1",
      "^@app/schemas(|/.*)$": "<rootDir>/libs/schemas/src/$1",
      "^@app/user-module(|/.*)$": "<rootDir>/libs/user-module/src/$1",
      "^@app/group-module(|/.*)$": "<rootDir>/libs/group-module/src/$1",
      "^@app/counter-module(|/.*)$": "<rootDir>/libs/counter-module/src/$1",
      "^@app/model-definition(|/.*)$": "<rootDir>/libs/model-definition/src/$1",
      "^@app/mock(|/.*)$": "<rootDir>/libs/mock/src/$1",
      "^@app/jwt(|/.*)$": "<rootDir>/libs/jwt/src/$1",
      "^@app/api-response(|/.*)$": "<rootDir>/libs/api-response/src/$1",
      "^@app/validation-pipe(|/.*)$": "<rootDir>/libs/validation-pipe/src/$1",
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/interface(|/.*)$": "<rootDir>/libs/interface/src/$1",
      "^@app/common/dto-middleware(|/.*)$": "<rootDir>/libs/common/dto-middleware/src/$1",
      "^@app/factory(|/.*)$": "<rootDir>/libs/factory/src/$1",
      "^@app/mq(|/.*)$": "<rootDir>/libs/mq/src/$1",
      "^@app/middleware(|/.*)$": "<rootDir>/libs/middleware/src/$1",
      "^@app/keypair(|/.*)$": "<rootDir>/libs/keypair/src/$1",
      "^@app/status(|/.*)$": "<rootDir>/libs/status/src/$1",
      "^@app/pusher-event(|/.*)$": "<rootDir>/libs/pusher-event/src/$1",
      "^@app/redis-utils(|/.*)$": "<rootDir>/libs/redis-utils/src/$1",
      "^./mqtt(|/.*)$": "<rootDir>/libs/mqtt/src/$1",
      "^@app/mqtt(|/.*)$": "<rootDir>/libs/mqtt/src/$1"
    }
  }
}